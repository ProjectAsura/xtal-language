{
	temp : 0;
	try{
		try{
			try{
				throw "test";
			}
			finally{
				temp = "finally";
			}
		}
		finally{
			temp ~= "2";
		}
	}
	catch(e){
		assert (e.message~temp)=="testfinally2";
	}
}


