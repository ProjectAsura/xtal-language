
{
	temp : 0;
	while(1){
		try{
			break;
		}
		finally{
			temp = 100;
		}
	}
	
	assert temp==100;
}


{
	temp : 0;
	e : fun(){
		try{
			throw "throw";	
		}finally{
			temp = 9999;
		}
	}

	try{
		e();
	}
	catch(e){
		assert e.message=="throw";
	}
	finally{
		temp += 1;
	}
	
	assert temp==10000;
}


