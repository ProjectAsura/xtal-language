inherit(lib::test);

nod : class{

}

mod : class(nod){
	foo : fun(){
		return "foo";
	}
}

inherit(mod);

nod::bar : fun(){
	return "bar";
}

mod::bar : fun(){
	return "mod::bar";
}

bar : fun(){
	return "bar";
}

class ToplevelTest{
	func#Test{
		assert foo===mod::foo;
		assert foo()=="foo";
		assert bar()=="bar";
	}
}

