
{
	foo: fun(){ return 0, 1, 2, 3, 4; }
	a, b: foo();
	assert a==0;
	assert b==[1, 2, 3, 4];
}

{
	foo: fun(){ return [0, 1, 2, 3, 4]; }
	a, b: foo();
	assert a==0;
	assert b==[1, 2, 3, 4];
}

{
	foo: fun(){ return 0, 1, 2, 3, 4; }
	a, b,: foo();
	assert a==0;
	assert b==1;
}

{
	a, b, c: 0, [1, 2];
	assert a==0;
	assert b==1;
	assert c==2;
}

{
	a, b, c: 0, [1, 2, 3];
	assert a==0;
	assert b==1;
	assert c==[2, 3];
}

{
	a, b, c,: 0, [1, 2, 3];
	assert a==0;
	assert b==1;
	assert c==2;
}

{
	a, b, c: 0, 1;
	assert a==0;
	assert b==1;
	assert c==null;
}

