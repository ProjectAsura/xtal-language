

Foo: class{
	+ _pub: 100;
	# _pro: 200;
	- _pri: 300; 
}

Bar: class(Foo){
	
}

{
	a: Foo();
	assert a.pub==100;
	
	a.pub = 10;
	assert a.pub==10;
	
	a.pub += 10;
	assert a.pub==20;
	
	temp: null;
	
	try{
		a.pro = 10;
	}catch(e){
		temp = "pro";
	}
	assert temp=="pro";
	
	try{
		a.pri = 10;
	}catch(e){
		temp = "pri";
	}
	assert temp=="pri";
	
	
	b: Bar();
	assert b.pub==100;
	
	Bar::pub: method(){ return 200; }
	assert b.pub==200;
	
	try{
		b.pro = 10;
	}catch(e){
		temp = "pro";
	}
	assert temp=="pro";
	
	Bar::pro: method(){ return 200; }
	assert b.pro==200;
}


