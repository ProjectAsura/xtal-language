
{
	fib : fiber(n){
		for(i : 0; i<n; ++i){
			yield i;
		}
	}
	
	assert fib(5)==0;
	assert fib()==1;
	assert fib()==2;
	assert fib()==3;
	assert fib()==4;
}

{
	fib : fiber{
		for(i : 0; i<5; ++i){
			yield i;
		}
	}
	assert fib.each.join(",")=="0,1,2,3,4";
}

{
	fib : fiber(n){
		f : 1;
		i : 1;
		j : 0;
		for(k : 0; k < n; ++k){
			yield f;
			f = i + j;
			j, i = i, f;
		}
	}
	
	assert !fib.is_running;
	
	assert fib(10)==1;
	
	assert fib.is_running;
	
	assert fib()==1;
	assert fib()==2;
	assert fib()==3;
	assert fib()==5;
	assert fib()==8;
	assert fib()==13;
	assert fib()==21;
	assert fib()==34;
	assert fib()==55;
	
}

