
nod : class{

}

mod : class(nod){
	foo : fun(){
		return "foo";
	}
}

filelocal.inherit(mod);

assert foo===mod::foo;

assert foo()=="foo";

nod::bar : fun(){
	return "bar";
}

assert bar()=="bar";

mod::bar : fun(){
	return "mod::bar";
}

assert bar()=="mod::bar";

filelocal::bar : fun(){
	return "filelocal::bar";
}

assert bar()=="filelocal::bar";

