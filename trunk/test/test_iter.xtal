{
	n : 0;
	10.times{ |v|
		n += v;
	}
	assert n==45;
}

{
	ret : range(5, 10).each.collect(fun(x) x*2).join(",");
	assert ret=="10,12,14,16,18";
}

{
	ret : [5, 8, 7, 3, 2].each.select(fun(x) x%2 != 0).join(",");
	assert ret=="5,7,3";
}

{
	str : "";
	[5, 8, 7, 3, 2]{ str ~= it.to_s; }
	assert str=="58732";
}


{
	n : 0;
	outer : 10.times{
		20.times{
			n++;
			break outer;
		}
	}
	assert n==1;
}

