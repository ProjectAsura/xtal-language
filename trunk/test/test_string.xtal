inherit(lib::test);

class StringTest{
	simple#Test{
		assert "test".length==4;
		assert ("te"~"st").length==4;
		assert ("te"~"st")=="test";
		assert "4".to_i==4;
	}
	
	sub#Test{
		assert "Hello".sub("l", fun(x) "L")=="HeLlo";
		assert "Hello".gsub("l", fun(x) "L")=="HeLLo";
		assert "Hello".sub("l"*2, fun(x) "O")=="HeOo";
	}
}
